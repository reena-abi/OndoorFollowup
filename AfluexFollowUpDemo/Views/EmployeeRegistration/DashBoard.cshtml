@model AfluexFollowUpDemo.Models.Lead

@{
    ViewBag.Title = "DashBoard";
    Layout = "~/Views/Shared/_UserLayout.cshtml";
}


<h6 class="mb-0 text-uppercase">Today's Followup</h6>
<hr />
<div class="card">
    <div class="card-body">
        <div class="table-responsive">
            <table id="example" class="table table-striped table-bordered" style="width:100%">
                <thead>
                    <tr>
                        <th>Contact Person</th>
                        <th>Contact No.</th>
                        <th>Interaction Date</th>
                        <th>Previous Followup Date</th>
                        <th>Followup Date</th>
                        <th>Expected Product</th>
                        <th>Data Source</th>
                        <th>Executive Name</th>
                        <th>Mode Of Interaction</th>
                        <th>Description </th>
                    </tr>
                </thead>
                <tbody>
                                        @if (Model != null)
                                        {
                                            if (Model.lstLead != null)
                                            {
                                                foreach (var item in Model.lstLead)
                                                {
                                                    <tr>
                                                   
                                                        <td>@item.Fk_ProcpectId</td>
                                                        <td>@item.ContactNo</td>
                                                        <td>@item.FirstInstructionDate</td>
                                                        <td>@item.PrevioussDate</td>
                                                        <td>@item.FollowupDate</td>
                                                        <td>@item.Fk_ExpectedProductCategoryId</td>
                                                        <td>@item.Fk_SourceId</td>
                                                        <td>@item.Fk_ExecutiveId</td>
                                                        <td>@item.Fk_ModeInterActionId</td>
                                                        <td>@item.Description</td>
                                                  
                                </tr>

                            }
                        }

                    }

                </tbody>

            </table>
        </div>
    </div>
</div>
<h6 class="mb-0 text-uppercase">Next Followup</h6>
<hr />
<div class="card">
    <div class="card-body">
        <div class="table-responsive">
            <table id="example1" class="table table-striped table-bordered ">
                <thead>
                    <tr>
                        <th>Contact Person</th>
                        <th>Contact No.</th>
                        <th>Interaction Date</th>
                        <th>Previous Followup Date</th>
                        <th>Followup Date</th>
                        <th>Expected Product</th>
                        <th>Data Source</th>
                        <th>Executive Name</th>
                        <th>Mode Of Interaction</th>
                        <th>Description </th>
                    </tr>
                </thead>
                <tbody>
                    @if (Model != null)
                    {
                        if (Model.lstnextLead != null)
                        {
                            foreach (var item in Model.lstnextLead)
                            {
                                <tr>
                                    <td>@item.Fk_ProcpectId</td>
                                    <td>@item.ContactNo</td>
                                    <td>@item.FirstInstructionDate</td>
                                    <td>@item.PrevioussDate</td>
                                    <td>@item.FollowupDate</td>
                                    <td>@item.Fk_ExpectedProductCategoryId</td>
                                    <td>@item.Fk_SourceId</td>
                                    <td>@item.Fk_ExecutiveId</td>
                                    <td>@item.Fk_ModeInterActionId</td>

                                    <td>@item.Description</td>
                                </tr>
                            }
                        }
                    }


                </tbody>

            </table>
        </div>
    </div>
</div>

<h6 class="mb-0 text-uppercase">Pending Followup</h6>
<hr />
<div class="card">
    <div class="card-body">
        <div class="table-responsive">
            <table id="example2" class="table table-striped table-bordered">
                <thead>
                    <tr>
                        <th>Contact Person</th>
                        <th>Contact No.</th>
                        <th>Interaction Date</th>
                        <th>Previous Followup Date</th>
                        <th>Followup Date</th>
                        <th>Expected Product</th>
                        <th>Data Source</th>
                        <th>Executive Name</th>
                        <th>Mode Of Interaction</th>
                        <th>Description </th>
                    </tr>
                </thead>
                <tbody>
                    @if (Model != null)
                    {
                        if (Model.lstpending != null)
                        {
                            foreach (var item in Model.lstpending)
                            {

                                <tr>
                                    <td>@item.Fk_ProcpectId</td>
                                    <td>@item.ContactNo</td>
                                    <td>@item.FirstInstructionDate</td>
                                    <td>@item.PrevioussDate</td>
                                    <td>@item.FollowupDate</td>
                                    <td>@item.Fk_ExpectedProductCategoryId</td>
                                    <td>@item.Fk_SourceId</td>
                                    <td>@item.Fk_ExecutiveId</td>
                                    <td>@item.Fk_ModeInterActionId</td>
                                    <td>@item.Description</td>
                                </tr>
                            }
                        }
                    }

                </tbody>

            </table>
        </div>
    </div>
</div>

